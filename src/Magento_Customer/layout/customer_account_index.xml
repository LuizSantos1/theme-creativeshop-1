<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="dashboard" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>

        <referenceContainer name="content" after="sticky.menu.wrapper">

            <container name="dashboard.page.main.title" htmlTag="div" htmlClass="cs-dashboard__page-title" before="customer_account_dashboard_top">
                <referenceBlock class="Magento\Theme\Block\Html\Title" name="page.main.title" template="html/title.phtml">
                    <action method="setPageTitle">
                        <argument translate="true" name="title" xsi:type="string">My Account</argument>
                    </action>
                </referenceBlock>
            </container>

        </referenceContainer>
    </body>
</page>

